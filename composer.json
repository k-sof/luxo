{
  "name": "luxo/website",
  "license": "proprietary",
  "type": "project",
  "require": {
    "php": "^7.2",
    "doctrine/annotations": "^1.8",
    "doctrine/orm": "^2.7",
    "monolog/monolog": "^2.0",
    "symfony/asset": "^5.0",
    "symfony/cache": "^5.0",
    "symfony/config": "^5.0",
    "symfony/console": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/dotenv": "^5.0",
    "symfony/form": "^5.0",
    "symfony/http-foundation": "^5.0",
    "symfony/mailer": "^5.1",
    "symfony/routing": "^5.0",
    "symfony/security-core": "^5.0",
    "symfony/translation": "^5.0",
    "symfony/twig-bridge": "^5.0",
    "symfony/validator": "^5.0",
    "twig/twig": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Luxo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Luxo\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.2.28"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "nelmio/alice": "^3.6",
    "sensiolabs/security-checker": "^6.0",
    "symfony/debug": "^4.4",
    "symfony/error-handler": "^5.0",
    "symfony/var-dumper": "^5.0"
  }
}
